#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>
//using namespace std;

int nThreads, n1, n2;


int *mdc (){
   int  resto;
   
   resto = n1%n2;
       
              while(resto!=0)
              {
                  n1    = n2;
                  n2    = resto;
                  resto = n1%n2;        
              }
              return n2;
}

void criaThreads (){
    long i;
    for (i = 0; i < nThreads; i++)
    {
       pthread_create(&thread[i], NULL, *mdc, (void *) &i);
    }
    
    for (i = 0; i < nThreads; i++)
    {
       pthread_join(thread[i], NULL);
    }
}

int main (int argc, char **argv){
     
   int i, j, qtdEquipe, qtdMembro;
   
   scanf ("%i %i", &qtdEquipe, &qtdMembro);
   nThreads = qtdMembro;
   pthread_t thread[nThreads];
   
   int array[qtdMembro*qtdEquipe];
   
      for (i = 0;i < qtdEquipe;i++) //denota a equipe
      { 
          for (j = 0;j < qtdMembro;j++) //denota o membro
         {
            scanf("%i %i", &n1, &n2);
            array[i+j] = (int) mdc(n1, n2);
      }
   }
         
         criarThreads();
         
   return 0;
}

