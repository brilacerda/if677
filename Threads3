#include <stdio.h>
#include <stdlib.h>
#include <pthread.h>

int nArquivos, nThreads, nProdutos;
char textbuf[4096];
char line[256];
main(){
	
	scanf("%d %d %d", &nArquivos, &nThreads, &nProdutos);

	int i;
	for(i = 0; i < nArquivos; i++){
		FILE * pFile;
	  	pFile = fopen ("1.in", "r");
	  	if (pFile!=NULL)
	  	{
			fclose (pFile);
		} else {
			fread(textbuf, line, 1, pFile);
		}
	}
	return 0;

}
